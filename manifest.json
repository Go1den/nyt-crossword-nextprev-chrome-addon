{
    "manifest_version": 2,
    "name": "NYT Crossword Navigator",
    "description": "Adds previous and next buttons that are inexplicably missing from this barebones website.",
    "version": "0.0.1",
    "icons": {
        "16" : "icons/icon-16.png",
        "48" : "icons/icon-48.png",
        "128" : "icons/icon-128.png"
    },
    "content_scripts": [{
        "matches": ["http://*.nytimes.com/crosswords/game/daily/*", "https://*.nytimes.com/crosswords/game/mini/*"],
        "js" : ["content-script.js"]
    }]
}